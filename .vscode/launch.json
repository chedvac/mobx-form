  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Jest Tests",
      "type": "node",
      "request": "launch",
      "runtimeArgs": ["--inspect=5858"],
      "runtimeExecutable":
        "${workspaceRoot}/node_modules/.bin/react-app-rewired",
      "env": {},
      "args": ["test", "--runInBand", "--no-cache", "--env=jsdom"],
      "protocol": "inspector",
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
