{
  "name": "formsinfra",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.0.1",
    "@material-ui/icons": "^3.0.1",
    "axios": "^0.18.0",
    "eslint": "^5.5.0",
    "lodash": "^4.17.10",
    "mobx": "^4.1.1",
    "mobx-react": "^5.0.0",
    "mobx-state-tree": "^2.0.3",
    "moment": "^2.22.2",
    "react": "^16.4.0",
    "react-autocomplete": "^1.8.1",
    "react-autocomplete-input": "^1.0.8",
    "react-day-picker": "^7.1.9",
    "react-dom": "^16.4.0",
    "react-html-id": "^0.1.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-textarea-autosize": "^6.1.0",
    "typeface-arimo": "0.0.54"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./public/css --include-path ./node_modules public/css/ -o public/css/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./public/css --include-path ./node_modules public/css/ -o public/css/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p  start-js",
    "build-js": "react-app-rewired build",
    "build": "npm-run-all build-css build-js",
    "jest": "jest --include-path ./_tests_ ",
    "test": "react-app-rewired test",
    "eject": "react"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-emotion": "^9.1.0",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-mobx": "^1.0.2",
    "check-prop-types": "^1.1.2",
    "enzyme": "^3.3.0",
    "es2015": "0.0.0",
    "eslint-plugin-jest": "^21.17.0",
    "jest": "^22.4.3",
    "mobx-react-devtools": "^5.0.1",
    "npm-run-all": "^4.1.3",
    "react-app-rewire-mobx": "^1.0.8",
    "react-app-rewire-preact": "^1.0.1",
    "react-app-rewired": "^1.5.0",
    "react-scripts": "^1.1.4"
  },
  "jest": {
    "moduleNameMapper": {
      "^_Demo(.*)$": "<rootDir>/src/_Demo$1",
      "^validations(.*)$": "<rootDir>/src/functional-validations/src$1",
      "^vmValidations(.*)$": "<rootDir>/src/vm-validations/src$1",
      "^core(.*)$": "<rootDir>/src/Mobx-MVVM/src$1",
      "^utils(.*)$": "<rootDir>/src/utils/src$1",
      "^resources(.*)$": "<rootDir>/src/resources$1",
      "^mobxReactForm(.*)$": "<rootDir>/src/mobx-react-form/src$1",
      "^react-ui-components(.*)$": "<rootDir>/src/react-ui-components/src$1"
    }
  }
}
